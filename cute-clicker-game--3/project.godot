;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Cute clicker game :3"
config/description="cute 2d clicker game "
config/version="1.0"
run/main_scene="res://game/game.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/image="res://ui/textures/sprite/Rabbit/RabbitPortrait.png"
config/icon="res://ui/textures/sprite/Rabbit/RabbitPortrait.png"

[display]

window/size/viewport_width=600
window/size/viewport_height=350
window/size/resizable=false
window/size/borderless=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
